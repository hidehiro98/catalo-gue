<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <h2>Create catalogue</h2>
      <%= simple_form_for @catalogue do |f| %>
        <%= f.input :rank1 %>
        <%= f.input :rank2 %>
        <%= f.input :rank3 %>
        <%= f.input :rank4 %>
        <%= f.input :rank5 %>
        <%= f.input :rank6 %>
        <%= f.input :rank7 %>
        <%= f.submit "Create catalogue", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
